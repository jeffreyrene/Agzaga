<% if @order.ffa_chapter.present? %>
  <div class="thank-you-heading" style="text-align: center; width: 600px; margin: 0 auto;">
    <img src="https://agazon-storage.nyc3.cdn.digitaloceanspaces.com/public_images/thank-you_header.png" style="width: 100%"></img>
  </div>

  <div
  style="
  text-align: center;
  border-bottom: 1px solid #000;
  line-height: 0.1em;
  margin: 0 auto;
  max-height: 2px;
  background: #004C97;
  max-width: 600px;
  margin-top: 30px;
  margin-bottom: 20px;">

  <span style=" font-family: Poppins-bold,  sans-serif;
                font-style: normal;
                font-weight: bold;
                font-size: 24px;
                align-items: center;
                letter-spacing: 0.03em;
                color: #000000;
                background-color: #fff;
                padding: 0 8px 0 46px;
                margin: 10px auto 20px">
    Your Contributed
  </span>

  <span style=" padding: 0 46px 0 0;
                background-color: #fff;
                color: #FFB514;
                align-items: center;
                letter-spacing: 0.03em;
                font-size: 24px;
                font-style: normal;
                font-weight: bold;
                font-family: Poppins-bold, sans-serif;">
    $<%= @order.five_percent_of_items_total %>
  </span>
</div>

<div style="  width: 100%;
              margin: 0 auto;
              text-align: center;
              font-family: Poppins, sans-serif;
              font-style: normal;
              font-weight: normal;
              font-size: 12px;
              line-height: 17px;
              text-align: center;
              letter-spacing: 0.03em;
              color: #000000;">
  to <span style="text-transform: uppercase;"><%= "#{@order.ffa_chapter.name}" %></span>
</div>

<div style="border:2px dashed #DADADA; width: 600px; margin: 20px auto; text-align: center;"></div>

<% else %>
  <div class="thank-you-heading" style="font-family: Draught, sans-serif;font-style: normal;font-weight: normal;font-size: 45px;line-height: 46px; align-items: center; text-align: center; letter-spacing: 0.03em; color: #000000; margin-bottom: 16px; cursor: default;padding: 0 45px;">
    Thank you for your order!
  </div>
<% end %>

<p class="sub-head-text" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 12px;line-height: 17px;align-items: center;text-align: center;letter-spacing: 0.03em;color: #000000;max-width: 384px;margin: 0 auto 24px;padding: 0 34px;cursor: default;">
  Take a look at these details, and keep them for your records. Weâ€™ll send you another email when we ship your order.
</p>

<div class="order-group"
      style="max-width: 536px;
              background: #FAFAFA;
              box-shadow: 0px 10px 20px rgba(0, 0, 0, 0.04), 0px 2px 6px rgba(0, 0, 0, 0.04), 0px 0px 1px rgba(0, 0, 0, 0.04);
              border-radius: 16px;
              margin: 0 auto 16px;
              padding: 30px 0 30px 0;">

  <div class="summary-heading-group" style="display: flex;align-items: center;margin-bottom: 16px;">

    <div class="gold-box" style="width: 16px;height: 16px;background: #F1B71C;margin-right: 16px;"></div>

    <p class="heading poppins-bold-text-18" style="font-family: Poppins-bold, sans-serif;font-style: normal;font-weight: bold;font-size: 18px;line-height: 23px;cursor: default;color: #171717;letter-spacing: 0.02em;margin: 0;">Order Summary</p>
  </div>

  <% @order.line_items.each do |item| %>
    <table style="margin: 0 12px 16px 12px; width: 96%">
      <tr style="margin-bottom: 3px;">
        <td style="font-family: Poppins, sans-serif;font-style: normal;font-weight: bold;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default;max-width: 360px;">
          <%= item.variant.product.name %>
        </td>

        <td style="font-family: Poppins, sans-serif;font-style: normal;font-weight: bold;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default; text-align: right;">
          <%= item.display_amount %>
        </td>
      </tr>

      <tr>
        <td style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 10px;line-height: 14px;letter-spacing: 0.03em;color: #000000;cursor: default;">
          <%= item.variant.options_text %>
        </td>

        <td style="text-align: right;">
          <p style="font-family: sans-serif;font-weight: normal;font-size: 8px;line-height: 11px;text-align: right;letter-spacing: 0.03em;color: #000000;cursor: default;font-style: italic; margin: 0;">
            SKU: <span><%= item.variant.sku %></span>
          </p>

          <div style="font-family: sans-serif;font-weight: normal;font-size: 8px;line-height: 11px;text-align: right;letter-spacing: 0.03em;color: #000000;cursor: default;font-style: italic; margin: 0;">
            Qty: <span><%= item.quantity %></span>
          </div>
        </td>
      </tr>
    </table>
  <% end %>

  <div class="gray-line-2x" style="border-bottom: 2px solid #EDEDED;margin-bottom: 16px;"></div>

  <table class="order-price-group" style="width: 96%; margin: 0 12px 16px 12px;">
    <tr style="margin: 0 12px 4px 12px;">
      <td class="subtotal-label poppins-text-12" style="font-family: Poppins, sans-serif; font-style: normal; font-weight: normal;font-size: 12px; line-height: 17px; letter-spacing: 0.03em; color: #000000; cursor: default;">
        <%= t('.subtotal') %>
      </td>

      <td class="sub-price poppins-text-12" style=" font-family: Poppins, sans-serif; font-style: normal; font-weight: normal;font-size: 12px; line-height: 17px; letter-spacing: 0.03em; color: #000000; cursor: default; text-align: right;">
        <%= @order.display_item_total %>
      </td>
    </tr>

    <% if @order.line_item_adjustments.exists? %>
      <% if @order.all_adjustments.promotion.eligible.exists? %>
        <% @order.all_adjustments.promotion.eligible.group_by(&:label).each do |label, adjustments| %>
          <tr class="subtotal-group" style="margin: 0 12px 4px 12px;">
            <td class="subtotal-label poppins-text-12" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default;">
              <%= label %>:
            </td>

            <td class="sub-price poppins-text-12" style="font-family: Poppins, sans-serif; font-style: normal; font-weight: normal;font-size: 12px; line-height: 17px; letter-spacing: 0.03em; color: #000000; cursor: default; text-align: right;">
              <%= Spree::Money.new(adjustments.sum(&:amount), currency: @order.currency) %>
            </td>
          </tr>
        <% end %>
      <% end %>
    <% end %>

    <% @order.shipments.group_by { |s| s.selected_shipping_rate.try(:name) }.each do |name, shipments| %>
      <tr class="subtotal-group" style="margin: 0 12px 4px 12px;">
        <td class="subtotal-label poppins-text-12" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default;">
          <%= name %>:
        </td>

        <td class="sub-price poppins-text-12" style="font-family: Poppins, sans-serif; font-style: normal; font-weight: normal; font-size: 12px; line-height: 17px; letter-spacing: 0.03em; color: #000000; cursor: default; text-align: right;">
          <%= Spree::Money.new(shipments.sum(&:total_before_tax), currency: @order.currency) %>
        </td>
      </tr>
    <% end %>

    <% if @order.all_adjustments.eligible.tax.exists? %>
      <% @order.all_adjustments.eligible.tax.group_by(&:label).each do |label, adjustments| %>
        <tr class="subtotal-group" style="margin: 0 12px 4px 12px;">
          <td class="subtotal-label poppins-text-12" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default;">
            <%= label %>:
          </td>

          <td class="sub-price poppins-text-12" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default; text-align: right;">
            <%= Spree::Money.new(adjustments.sum(&:amount), currency: @order.currency) %>
          </td>
        </tr>
      <% end %>
    <% end %>

    <% @order.adjustments.eligible.each do |adjustment| %>
      <% next if (adjustment.source_type == 'Spree::TaxRate') and (adjustment.amount == 0) %>

      <tr class="subtotal-group" style="display: flex;margin: 0 12px 4px 12px;">
        <td class="subtotal-label poppins-text-12" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default;">
          <%= adjustment.label %>:
        </td>

        <td class="sub-price poppins-text-12" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default; text-align: right;">
          <%= adjustment.display_amount %>
        </td>
      </tr>
    <% end %>
    <% if @order.total_applicable_store_credit > 0 %>
      <tr class="subtotal-group" style="margin: 0 12px 4px 12px;">
        <td class="subtotal-label poppins-text-12" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default;">
            <%= t('spree.store_credit.store_credit') %>:
        </td>

        <td class="sub-price poppins-text-12" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: normal;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default; text-align: right;">
          <%= @order.display_total_applicable_store_credit.to_html %>
        </td>
      </tr>
    <% end %>

    <tr class="subtotal-group" style="margin: 0 12px 4px 12px;">
      <td class="subtotal-label poppins-text-12 bold" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: 700;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default;">
        <%= t('.total') %>
      </td>

      <td class="sub-price poppins-text-12 bold" style="font-family: Poppins, sans-serif;font-style: normal;font-weight: 700;font-size: 12px;line-height: 17px;letter-spacing: 0.03em;color: #000000;cursor: default; text-align: right;">
        <%= @order.display_total %>
      </td>
    </tr>
  </table>
</div>

